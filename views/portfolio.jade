extends layout

block content
  body
    div#portfolioWrapper
        div.nav
            ul#navOptions
                li(onclick="window.location.href='/'") home
                li(onclick="window.location.href='/about'") about me
                li(onclick="window.location.href='/recent'") recent
        section#photos
            each image, i in images
                div#advancedImageContainer
                    img(src="images/portfolio/#{image}")